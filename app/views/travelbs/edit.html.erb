<h1>Edit Form</h1>

<%= form_for @travelblog, url:travelb_path, method: :PATCH do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
    <% @travelblog.errors.full_messages_for(:name).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :place_to_visit %><br>
    <%= form.text_area :place_to_visit %>
    <% @travelblog.errors.full_messages_for(:place_to_visit).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :contact_number %><br>
    <%= form.text_field :contact_number %>
    <% @travelblog.errors.full_messages_for(:contact_number).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :email_id %><br>
    <%= form.text_field :email_id %>
    <% @travelblog.errors.full_messages_for(:email_id).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>